      ]
    },
    {
      "parameters": {
        "jsCode": "// Prepare document for upload\nconst item = $input.first().json;\n\nconst uploadRequest = {\n  document_type: item.document_type,\n  title: item.title,\n  content: item.content,\n  file_name: item.file_name || `Document_${new Date().toISOString().split('T')[0]}.md`,\n  format: item.format || 'markdown',\n  metadata: item.metadata || {},\n  visuals: item.visuals || [],\n  output_folder_id: item.output_folder_id || item.config?.output_folder_id || '1a2b3c4d5e6f'\n};\n\nreturn [{ json: uploadRequest }];"
      },
      "id": "prepare-upload",
      "name": "Prepare Upload",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        220,
        0
      ]
    },
    {
      "parameters": {
        "jsCode": "// Convert markdown content to file binary\nconst item = $input.first();\nconst content = item.json.content;\nconst fileName = item.json.file_name;\n\n// Create binary data from content\nconst binaryData = Buffer.from(content, 'utf-8').toString('base64');\n\nreturn [{\n  json: item.json,\n  binary: {\n    data: {\n      data: binaryData,\n      mimeType: 'text/markdown',\n      fileName: fileName\n    }\n  }\n}];"
      },
      "id": "create-binary",
      "name": "Create Binary Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        440,
        0
      ]
    },
    {
      "parameters": {
        "operation": "upload",
        "name": "={{ $json.file_name }}",
        "folderId": {
          "__rl": true,
          "mode": "id",
          "value": "={{ $json.output_folder_id }}"
        },
        "options": {
          "simplifyOutput": true
        }
      },
      "id": "gdrive-upload",
      "name": "Google Drive - Upload",
      "type": "n8n-nodes-base.googleDrive",
      "typeVersion": 3,
      "position": [
        660,
