    },
    {
      "parameters": {
        "jsCode": "// Format success response\nconst item = $input.first();\n\nreturn [{\n  json: {\n    status: 'uploaded',\n    file_id: item.json.id,\n    file_name: item.json.name,\n    web_link: item.json.webViewLink,\n    folder_id: item.json.parents?.[0],\n    uploaded_at: new Date().toISOString()\n  }\n}];"
      },
      "id": "format-success",
      "name": "Format Success",
