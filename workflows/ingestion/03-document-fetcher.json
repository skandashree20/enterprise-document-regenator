{
  "name": "03 - Document Fetcher",
  "nodes": [
    {
      "parameters": {},
      "id": "trigger-node",
      "name": "Execute Workflow Trigger",
      "type": "n8n-nodes-base.executeWorkflowTrigger",
      "typeVersion": 1,
      "position": [0, 0]
    },
    {
      "parameters": {
        "jsCode": "// Extract file info from input\nconst fileId = $input.first().json.file_id;\nconst fileName = $input.first().json.file_name;\nconst mimeType = $input.first().json.mime_type;\nconst extension = $input.first().json.extension;\n\nif (!fileId) {\n  throw new Error('No file_id provided');\n}\n\nreturn [{\n  json: {\n    file_id: fileId,\n    file_name: fileName,\n    mime_type: mimeType,\n    extension: extension,\n    fetch_started: new Date().toISOString()\n  }\n}];"
      },
      "id": "prepare-fetch",
      "name": "Prepare Fetch",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [220, 0]
    },
    {
      "parameters": {
        "operation": "download",
        "fileId": {
          "__rl": true,
          "mode": "id",
          "value": "={{ $json.file_id }}"
        },
        "options": {}
      },
      "id": "gdrive-download",
      "name": "Google Drive - Download",
      "type": "n8n-nodes-base.googleDrive",
      "typeVersion": 3,
      "position": [440, 0],
      "credentials": {
        "googleDriveOAuth2Api": {
          "id": "CONFIGURE_ME",
          "name": "Google Drive OAuth2"
        }
      },
      "continueOnFail": true
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
